{% extends 'base.html.twig' %}

{% block body %}

    <div id="wrapper">
        <div id="container">
            <a href="/addForm">Add User</a>
            {% if users %}
             <ul>
                 {% for user in users %}
                     <li>{{ user.name|e }} {{ user.lastname|e }} <a href="/gUser/{{ user.id }}">Edit</a> | <a
                                 href="/deleteUser/{{ user.id }}"></a></li>
                 {% endfor %}
             </ul>
             {% endif %}
        </div>
    </div>
    
{% endblock body %}